    "product_name = input(\"Enter your product : \")\n",
    "\n",
    "user_agent = "Enter your own user agent",
    "\n",
    "# Path to your msedgedriver.exe\n",
    "edge_driver_path = os.path.join(os.getcwd(), 'msedgedriver.exe')\n",
    "\n",
    "# Create a Service object for Edge\n",
    "edge_service = Service(edge_driver_path)\n",
    "\n",
    "# Set up Edge options (e.g., user-agent)\n",
    "edge_options = Options()\n",
    "edge_options.add_argument(f'user-agent={user_agent}')\n",
    "\n",
    "# Initialize the Edge browser using correct WebDriver\n",
    "browser = Edge(service=edge_service, options=edge_options)\n",
    "browser.get('https://www.amazon.in/s?k='+product_name.replace(' ','+'))\n",
    "div_element = browser.find_element(By.CSS_SELECTOR, \"div[data-cy='title-recipe']\")\n",
    "inner_html = div_element.get_attribute('innerHTML')\n",
    "soup = BeautifulSoup(inner_html,'html.parser')\n",
    "target_classes = [\"a-size-medium a-color-base a-text-normal\", \"a-size-base-plus a-color-base a-text-normal\"]\n",
    "spans = soup.find_all('span', class_=target_classes)\n",
    "amazon_title = spans[0].get_text()\n",
    "#print(amazon_title)\n",
    "\n",
    "div_element = browser.find_element(By.CSS_SELECTOR, \"div[data-cy='price-recipe']\")\n",
    "inner_html = div_element.get_attribute('innerHTML')\n",
    "soup = BeautifulSoup(inner_html,'html.parser')\n",
    "amazon_price = soup.find('span',attrs={'class' : \"a-offscreen\"}).text\n",
    "#print(amazon_price)\n",
    "\n",
    "div_element = browser.find_element(By.CSS_SELECTOR, \"div[data-cy='reviews-block']\")\n",
    "inner_html = div_element.get_attribute('innerHTML')\n",
    "soup = BeautifulSoup(inner_html,'html.parser')\n",
    "amazon_rating = soup.find('span',attrs={'class' : \"a-icon-alt\"})\n",
    "if amazon_rating:\n",
    "    amazon_rating=amazon_rating.text\n",
    "    #print(amazon_rating)\n",
    "else:\n",
    "    print(\"No reviews for this item\")\n",
    "    \n",
    "browser.get('https://www.flipkart.com/search?q='+product_name.replace(' ','%20'))\n",
    "div_element = browser.find_element(By.CSS_SELECTOR, \"div.DOjaWF.YJG4Cf\")\n",
    "inner_html = div_element.get_attribute('innerHTML')\n",
    "soup = BeautifulSoup(inner_html,'html.parser')\n",
    "first_soup = soup.find(\"div\", attrs={'class' : \"DOjaWF gdgoEp\"})\n",
    "second_soup = first_soup.find(\"div\",attrs={'class':'cPHDOP col-12-12'})\n",
    "flipkart_title = second_soup.find(\"div\",attrs={'class':'KzDlHZ'})\n",
    "\n",
    "if flipkart_title == True or flipkart_title is not None:\n",
    "    flipkart_title=flipkart_title.text\n",
    "    #print(flipkart_title)\n",
    "  \n",
    "else:\n",
    "    flipkart_title= second_soup.find_all(\"a\")\n",
    "    flipkart_title=flipkart_title[1].text\n",
    "    #print(flipkart_title[1].text)\n",
    "\n",
    "div_element = browser.find_element(By.CSS_SELECTOR, \"div.DOjaWF.YJG4Cf\")\n",
    "inner_html = div_element.get_attribute('innerHTML')\n",
    "soup = BeautifulSoup(inner_html,'html.parser')\n",
    "target_classes = [\"Nx9bqj _4b5DiR\", \"Nx9bqj\"]\n",
    "spans = soup.find('div', class_=target_classes)\n",
    "flipkart_price = spans.get_text()\n",
    "#print(flipkart_price)\n",
    "\n",
    "div_element = browser.find_element(By.CSS_SELECTOR, \"div.DOjaWF.YJG4Cf\")\n",
    "inner_html = div_element.get_attribute('innerHTML')\n",
    "soup = BeautifulSoup(inner_html,'html.parser')\n",
    "first_soup = soup.find(\"div\", attrs={'class' : \"DOjaWF gdgoEp\"})\n",
    "second_soup = first_soup.find(\"div\",attrs={'class':'cPHDOP col-12-12'})\n",
    "flipkart_rating = second_soup.find(\"div\",attrs={'class':'XQDdHH'})\n",
    "\n",
    "if flipkart_rating == True or flipkart_rating is not None:\n",
    "    flipkart_rating=flipkart_rating.text\n",
    "    #print(flipkart_rating,\"out of 5.0\")\n",
    "else:\n",
    "    print(\"No rating Found\")\n",
    "\n",
    "amazon=[amazon_title,amazon_price,amazon_rating]\n",
    "flipkart=[flipkart_title,flipkart_price,flipkart_rating]\n",
    "df = pd.DataFrame({\n",
    "    'Amazon':amazon,\n",
    "    'Flipkart':flipkart\n",
    "})\n",
    "df.index = ['Title', 'Price', 'Rating']\n",
    "df"
   ]
